        function GridFetch() {

            $.ajax({//Get All Grid
                type: 'GET',
                url: '/GridFetch',
                async: true,
                cache: cache,
                dataType: 'json',
                success: function (data1) {
                    gridData = data1
                    Grid(gridData)
                    $('#GridLoader').jqxLoader('close');
                    addfilter(TodayDate);
                },
                error: function (xhr, status, errorThrown) {
                    console.log(xhr);
                    console.log(status);
                    console.log(errorThrown);
                }
            });
        }

